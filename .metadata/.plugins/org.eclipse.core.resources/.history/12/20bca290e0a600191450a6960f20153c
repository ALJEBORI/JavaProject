package com.hardis.application;

public class trianerOperator {

	public static void main(String[] args) {
		int age = 20;
		boolean vegetrian = true;
		if (vegetrian) {
			System.out.println("Salade");
		} else {
			System.out.println("Vache");
		}
		String state = (age < 18) ? "Minor" : "Major";
		System.out.println("state = " + state);

	}

}
